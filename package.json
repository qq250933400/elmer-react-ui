{
  "name": "web-frame-work",
  "version": "1.0.0",
  "description": "web-frame-work to use build website",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "better-npm-run dev",
    "build": "npm run clear && better-npm-run build",
    "clear": "rimraf build",
    "lint": "eslint src",
    "lng": "babel-node ./webpack/languageBuild.js",
    "lnglint": "eslint webpack",
    "cmd": "eslint ./src/builder && babel-node src/builder/index.js",
    "server": "babel-node ./server/dev-server.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "web",
    "frame",
    "website"
  ],
  "betterScripts": {
    "lint": "eslint src & eslint config & eslint webpack",
    "dev": {
      "command": "concurrently \"babel-node webpack/watch.js\"",
      "env": {
        "EXEC_ENV": "development",
        "NODE_ENV": "development",
        "RUN_TASK": "watch",
        "DEBUG": "app:*",
        "UN_THREADPOOL_SIZE": 100,
        "DEV_SERVER_PORT": 4000,
        "REACT_DEVTOOLS_PORT": 8097,
        "REDUX_DEVTOOLS_PORT": 8000
      }
    },
    "staging": {
      "command": "babel-node webpack/compile.js",
      "env": {
        "EXEC_ENV": "staging",
        "NODE_ENV": "production",
        "UV_THREADPOOL_SIZE": 100
      }
    },
    "build": {
      "command": "babel-node webpack/compile.js",
      "NODE_ENV": "production",
      "UV_THREADPOOL_SIZE": 100
    },
    "esdoc": {
      "command": "esdoc"
    },
    "jest": {
      "command": "jest"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better": "^0.1.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "elmer-react-event": "^1.0.0",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-redux-saga": "0.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "generator-eslint": "^1.2.0",
    "gutil": "^1.6.4",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^2.29.0",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "ncp": "^2.0.0",
    "npm-run": "^4.1.2",
    "node-sass": "^4.0.0",
    "postcss-less": "^1.1.0",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "stylish": "^1.0.0",
    "stylus": "0.40.0",
    "svg-react": "^1.0.9",
    "svg-url-loader": "^2.1.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-simple-progress-plugin": "^0.0.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "yo": "^2.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "devDependencies": {
    "eslint-plugin-babel": "^4.1.2",
    "remote-redux-devtools": "^0.5.12",
    "@storybook/react": "^3.2.8",
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-links": "^3.2.6"
  }
}
